<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="WhiteBox">
        <item name="android:background">@drawable/box</item>
    </style>
    <style name="BlackBox">
        <item name="android:background">@drawable/box</item>
        <item name="android:backgroundTint">#292929</item>
        <item name="android:color">#ebebeb</item>
        <item name="android:padding">18dp</item>
    </style>
    <style name="WhiteButton">
        <item name="android:backgroundTint">@android:color/white</item>
        <item name="android:tint">#4d4d4d</item>
    </style>
    <style name="FlatButton">
        <item name="android:background">@android:color/transparent</item>
    </style>
    <style name="FragmentTitle">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">20dp</item>
        <item name="android:layout_marginBottom">8dp</item>
    </style>
    <style name="FlatButtonLight">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:tint">#ebebeb</item>
    </style>
    <style name="FlatButtonLightSelected" parent="FlatButtonLight">
        <item name="android:tint">#64bde7</item>
    </style>
</resources>